<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Sign_with_Email">


    <Button
        android:id="@+id/button2"
        android:layout_width="33dp"
        android:layout_height="33dp"
        android:layout_marginStart="80dp"
        android:layout_marginLeft="80dp"
        android:layout_marginEnd="378dp"
        android:layout_marginRight="378dp"
        android:layout_marginBottom="15dp"
        android:background="@drawable/ic_left"
        app:layout_constraintBottom_toTopOf="@+id/textView8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:onClick="Back_Home"
        />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="80dp"
        android:layout_marginBottom="12dp"
        android:text="Create an Account "
        android:textColor="@color/black"
        android:textSize="30dp"
        app:layout_constraintBottom_toTopOf="@+id/linearLayoutCompat10"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.658"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <androidx.appcompat.widget.LinearLayoutCompat
        android:id="@+id/linearLayoutCompat10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="20dp"

        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@+id/linearLayoutCompat8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="5dp"
            android:text="Enter your name "
            android:textColor="@color/black"
            android:textSize="15dp"

            tools:layout_editor_absoluteX="46dp"
            tools:layout_editor_absoluteY="180dp" />

        <EditText
            android:id="@+id/editText3"
            android:layout_width="301dp"
            android:layout_height="wrap_content"
            android:background="@drawable/shape_plain_text"
            android:ems="10"
            android:hint="Name "
            android:inputType="textPersonName"
            android:padding="12dp"
            tools:layout_editor_absoluteX="45dp"
            tools:layout_editor_absoluteY="207dp" />


    </androidx.appcompat.widget.LinearLayoutCompat>

    <androidx.appcompat.widget.LinearLayoutCompat
        android:id="@+id/linearLayoutCompat8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="20dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@+id/linearLayoutCompat9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="5dp"
            android:text="Email "
            android:textColor="@color/black"
            android:textSize="15dp"
            tools:layout_editor_absoluteX="43dp"
            tools:layout_editor_absoluteY="287dp" />

        <EditText
            android:id="@+id/editText4"
            android:layout_width="301dp"
            android:layout_height="wrap_content"
            android:background="@drawable/shape_plain_text"
            android:ems="10"
            android:hint="Name"
            android:inputType="textPersonName"
            android:padding="12dp"
            tools:layout_editor_absoluteX="43dp"
            tools:layout_editor_absoluteY="314dp" />

    </androidx.appcompat.widget.LinearLayoutCompat>


    <androidx.appcompat.widget.LinearLayoutCompat
        android:id="@+id/linearLayoutCompat9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.558">

        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="5dp"
            android:text="Password"
            android:textColor="@color/black"

            android:textSize="15dp"
            tools:layout_editor_absoluteX="43dp"
            tools:layout_editor_absoluteY="403dp" />

        <EditText
            android:id="@+id/editText5"
            android:layout_width="301dp"
            android:layout_height="wrap_content"
            android:background="@drawable/shape_plain_text"
            android:ems="10"
            android:hint="Enter at least 6 characters "
            android:inputType="textPersonName"
            android:padding="12dp"
            tools:layout_editor_absoluteX="43dp"
            tools:layout_editor_absoluteY="422dp" />
    </androidx.appcompat.widget.LinearLayoutCompat>

    <Button
        android:id="@+id/button3"
        android:layout_width="301dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="23dp"
        android:background="@drawable/round_shape"
        android:padding="15dp"
        android:text="Sign Up "
        android:textColor="@color/white"
        app:layout_constraintBottom_toTopOf="@+id/textView3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.518"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayoutCompat9" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="55dp"
        android:layout_marginLeft="55dp"
        android:layout_marginTop="14dp"
        android:layout_marginEnd="59dp"
        android:layout_marginRight="59dp"
        android:layout_marginBottom="23dp"
        android:padding="15dp"
        android:text="@string/OR_Messsage"
        app:layout_constraintBottom_toTopOf="@+id/linearLayoutCompat3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button3" />

    <androidx.appcompat.widget.LinearLayoutCompat
        android:id="@+id/linearLayoutCompat3"
        android:layout_width="411dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="65dp"
        android:gravity="center"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <Button
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_margin="25dp"
            android:background="@drawable/ic_facebook">

        </Button>

        <Button
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_margin="2dp"
            android:background="@drawable/ic_sv_google"
            android:shadowColor="@color/end"

            ></Button>


        <Button
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_margin="25dp"
            android:background="@drawable/ic_mobile"></Button>

    </androidx.appcompat.widget.LinearLayoutCompat>

</androidx.constraintlayout.widget.ConstraintLayout>